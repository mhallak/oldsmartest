<!-- -*- mode: XML -*- -->
<launch>    


     <arg name="DivLim" default="1"/> 
     <arg name="SampNum" default="1"/> 
    
     <arg name="SFDP_file_url" value="$(find smartest)/work_space/root.SFDP"/> 
     <arg name="resource_file_url" value="$(find smartest)/resource/resource_srvssDumy.xml"/> 
     <arg name="work_space_folder_url" value="$(find smartest)/work_space/"/>

     <!-- setting PYTHON PATH variable to load Python Interfaces scripts -->
     <env name="PYTHONPATH" value="$(env PYTHONPATH):$(find smartest)/scripts"/>

     <node pkg="smartest" type="mainGen" name="SRVSS_Exploration_srvssDumy" args="-DomainEploretion $(arg SFDP_file_url) $(arg work_space_folder_url) $(arg resource_file_url) $(arg DivLim) $(arg ScenNum)" output="screen"/>

</launch>
