<!-- -*- mode: XML -*- -->
<launch>    

     <arg name="n" default="2"/> 
    
     <arg name="SFDP_file_url" value="$(find smartest)/SFDP/dummy.SFDP"/> 

     <arg name="resource_file_url" value="$(find smartest)/resource/resource_srvssDumy.xml"/> 
     <arg name="work_space_folder_url" value="$(find smartest)/work_space/"/>

     <!-- setting PYTHON PATH variable to load Python Interfaces scripts -->
     <env name="PYTHONPATH" value="$(env PYTHONPATH):$(find smartest)/scripts"/>

     <node pkg="smartest" type="mainGen" name="SRVSS_Multiple_Scenarios_GenRun" args="-MultipleScensGenRun $(arg SFDP_file_url) $(arg work_space_folder_url) $(arg resource_file_url) $(arg n)" output="screen"/>

</launch>
