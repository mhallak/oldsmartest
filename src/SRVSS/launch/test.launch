<!-- -*- mode: XML -*- -->
<launch>    


     <arg name="scen" default="scenario_1"/> 
    
      <arg name="SFDP_file_url" value="$(find SRVSS)/SFDP/test.SFDP"/> 
     <!-- <arg name="SFDP_file_url" value="$(find SRVSS)/SFDP/robil2.SFDP"/> -->
     <arg name="resource_file_url" value="$(find SRVSS)/resource/resource_srvssDumy.xml"/> 
     <!-- <arg name="resource_file_url" value="$(find SRVSS)/resource/resource_robil2.xml"/> -->
     <arg name="scenarios_folder_url" value="$(find SRVSS)/scenarios/$(arg scen)"/>


     <node pkg="SRVSS" type="MainTest" name="test_node" args="$(arg SFDP_file_url) $(arg scenarios_folder_url) $(arg resource_file_url)" output="screen" />


	
	




       
</launch>
